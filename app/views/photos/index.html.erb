<div class="container .table-responsive">
  <h2>All Photos</h2>
  <p>
    <button type="button" class="btn btn-default btn-lg">
      <%= link_to 'New Photo', new_photo_path %>  
    </button>
  </p>
  <table class="table table-hover table-bordered table-condensed">
  <% @photos.each do |photo| %>
    <tr class="table-spacing">
      <td><%= photo.title %></td>
      <td class="active"><%= link_to 'Show', photo_path(photo) %></td>
      <td class="warning"><%= link_to 'Edit', edit_photo_path(photo) %></td>
      <td class="danger"><%= link_to 'Destroy', photo_path(photo), method: :delete, data:{ confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
  </table>
</div>